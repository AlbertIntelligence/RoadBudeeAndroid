<fragment xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/map"
    android:name="com.google.android.gms.maps.SupportMapFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.codekl.roadbudee.Activities.MapsActivity">

<com.ramotion.circlemenu.CircleMenuView
    android:id="@+id/circle_menu"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:button_colors="@array/colors"
    android:button_icons="@array/icons"
    android:layout_constraintBottom_toBottomOf="parent"
    android:layout_constraintHorizontal_bias="0.5"
    android:layout_constraintLeft_toLeftOf="parent"
    android:layout_constraintRight_toRightOf="parent"
    android:layout_constraintTop_toTopOf="parent"/>

    </fragment>
